<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.auth.mapper.UserMapper">
    <select id="getRoleCodeListByUserId" resultType="java.lang.String">
        select code from sys_role r left join sys_user_role ur on ur.role_id = r.id where ur.user_id = #{userId}
    </select>
</mapper>